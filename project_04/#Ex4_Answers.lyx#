\begin_layout Standard
\begin_inset Formula $\left(b\right)\Rightarrow\left(a\right)$
\end_inset

 לפי הגדרת הגבול לכל 
\begin_inset Formula $\varepsilon,\delta>0$
\end_inset

 קיים 
\begin_inset Formula $m\left(\varepsilon,\delta\right)\in\mathbb{N}$
\end_inset

 כך שלכל 
\begin_inset Formula $m>m\left(\varepsilon,\delta\right)$
\end_inset

 מתקיים:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left|\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)\right]-0\right|\leq\varepsilon\cdot\delta\iff\left|\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)\right]\right|\leq\varepsilon\cdot\delta
\]

\end_inset


\end_layout

\begin_layout Standard
כעת, נשים לב כי מתקיים
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Pr_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)\leq\varepsilon\right]=1-\Pr_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)>\varepsilon\right]\overset{\text{Markov's Inequality}}{\geq}1-\frac{\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)\right]}{\varepsilon}\geq1-\frac{\varepsilon\cdot\delta}{\varepsilon}=1-\delta
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left(a\right)\Rightarrow\left(b\right)$
\end_inset

 יהי 
\begin_inset Formula $\varepsilon>0$
\end_inset

.
 מ-
\begin_inset Formula $\left(a\right)$
\end_inset

 קיים 
\begin_inset Formula $m\left(\frac{\varepsilon}{2},\frac{\varepsilon}{2}\right)$
\end_inset

 כך שלכל 
\begin_inset Formula $m>m\left(\frac{\varepsilon}{2},\frac{\varepsilon}{2}\right)$
\end_inset

 מתקיים
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Pr_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)\leq\frac{\varepsilon}{2}\right]\geq1-\frac{\varepsilon}{2}\implies\Pr_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)>\frac{\varepsilon}{2}\right]\leq\frac{\varepsilon}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
בנוסף נתון לנו שפונקציית ה-
\begin_inset Formula $Loss$
\end_inset

 שלנו היא 
\begin_inset Formula $L_{D}\left(A\left(S\right)\right)\in\left[0,1\right]$
\end_inset

 ולכן 
\begin_inset Formula 
\[
\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)|L_{D}\left(A\left(S\right)\right)\geq\frac{\varepsilon}{2}\right]\leq1\land\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)|L_{D}\left(A\left(S\right)\right)\leq\frac{\varepsilon}{2}\right]\leq\frac{\varepsilon}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
כעת, מנוסחאת התוחלת השלמה )משפט ההחלקה( נקבל:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\left|\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)\right]\right| & =\Big|\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)|L_{D}\left(A\left(S\right)\right)\geq\frac{\varepsilon}{2}\right]\cdot\Pr\left[L_{D}\left(A\left(S\right)\right)\geq\frac{\varepsilon}{2}\right]\\
 & +\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)|L_{D}\left(A\left(S\right)\right)\leq\frac{\varepsilon}{2}\right]\cdot\Pr\left[L_{D}\left(A\left(S\right)\right)\leq\frac{\varepsilon}{2}\right]\Big|\\
 & \leq\left|1\cdot\frac{\varepsilon}{2}+\frac{\varepsilon}{2}\cdot1\right|\\
 & =\varepsilon
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
ומכאן שמתקיים 
\begin_inset Formula $\lim_{m\rightarrow\infty}\mathbb{E}_{S\sim D^{m}}\left[L_{D}\left(A\left(S\right)\right)\right]=0$
\end_inset

 כנדרש.
\end_layout

\begin_layout Standard
\align left
\begin_inset Formula $\blacksquare$
\end_inset


\end_layout

