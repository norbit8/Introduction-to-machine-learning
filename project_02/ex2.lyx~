\begin_layout Standard
\begin_inset Formula 
\[
\vec{v}_{j}\cdot\vec{v}_{j}^{T}=\left[\begin{array}{c}
v_{j,1}\\
v_{j,2}\\
\vdots\\
v_{j,d}
\end{array}\right]\cdot\left[\begin{array}{cccc}
v_{j,1} & v_{j,2} & \dots & v_{j,d}\end{array}\right]=\left(\begin{array}{cccc}
\left(v_{j,1}\right)^{2} & \left(v_{j,1}\cdot v_{j,2}\right) & \cdots & \left(v_{j,1}\cdot v_{j,d}\right)\\
\left(v_{j,2}\cdot v_{j,1}\right) & \left(v_{j,2}\right)^{2} & \ddots & \vdots\\
\vdots & \ddots & \ddots & \vdots\\
\left(v_{j,d}\cdot v_{j,1}\right) & \left(v_{j,d}\cdot v_{j,2}\right) & \cdots & \left(v_{j,d}\right)^{2}
\end{array}\right)
\]

\end_inset


